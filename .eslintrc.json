{
  "ignorePatterns": ["const.ts"],
  "extends": [
      "next/core-web-vitals"
     
    ],
  "rules": {
      "no-console": "warn",
      "quotes": ["error", "double"],
      "jsx-quotes": ["error", "prefer-double"],
      "indent": ["warn", 2],
      "comma-dangle": ["error", "always-multiline"],
      "semi": ["warn", "always"],
      "max-len": ["error", {"code": 140}
    ]
  }
}